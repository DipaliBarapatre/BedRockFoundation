body{
	background:#fff url(<%= asset_path 'scheme2/bg.jpg' %>) repeat-x;
	}
	
th {
	background:#95bf84 url(<%= asset_path 'highlight_line.gif' %>) repeat-x scroll 0 0;
}

tr.alt td {
	background: #eff5ec url(<%= asset_path 'highlight_line.gif' %>) repeat-x scroll 0 0;
	color: #797268;
}

tr.alt th.spec {
	background: #eff5ec url(<%= asset_path 'scheme2/bullet1.gif' %>) no-repeat;
	color: #797268;
}

.button{
	background:#95bf84 url(<%= asset_path 'highlight_line.gif' %>) repeat-x top left;
}

.button:hover{
	background:#b1c5a8;
}

tr.endsection td {
border-bottom: 2px solid #000;
}